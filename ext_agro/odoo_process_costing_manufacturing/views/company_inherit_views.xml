<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="company_inherit_view_form_porcentaje" model="ir.ui.view">
        <field name="name">company.inherit.view.form.porcentaje</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
      

            <xpath expr="//group[@name='ret_provee']" position="after">
                    <group string="Configuracion de porcentaje de ganancia de ventas" name="porcentaje_ganancia_venta" aling="center">
                        <field name="porcentaje_ganancia" string="Porcentaje de Ganancia de Ventas (%):"/>
                    </group>
                </xpath>
        </field>
    </record>
</odoo>